{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Contacts Exporter for WhatsApp Marketing Bot",
  "short_name": "Contacts Exporter",
  "version": "1.0",
  "description": "Export numbers from WhatsApp and import into the WhatsApp Marketing Bot",
  "permissions": [ "declarativeContent", "downloads", "*://*.whatsapp.com/*", "storage" ],
  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Contacts Exporter"
  },
  "icons": {
    "16": "images/icon16.png",
    "19": "images/icon19.png",
    "32": "images/icon32.png",
    "38": "images/icon38.png",
    "48": "images/icon48.png",
    "96": "images/icon96.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 2
}
